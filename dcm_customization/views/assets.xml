<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_common" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <script src="https://code.highcharts.com/highcharts.js"></script>
            <script src="https://code.highcharts.com/modules/sunburst.js"></script>
<!--            <link rel="stylesheet" type="text/css" href="https://code.highcharts.com/css/highcharts.css"/>-->
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
        	<link rel="stylesheet" type="text/css" href="/dcm_customization/static/src/css/jqtree.css"/>
        	<link rel="stylesheet" type="text/css" href="/dcm_customization/static/src/css/jquery.orgchart.css"/>
            <!--<link rel="stylesheet" type="text/scss"
                  href="/dcm_customization/static/src/scss/midardir.scss"/>-->
            <script type="text/javascript" src="/dcm_customization/static/src/js/jquery.orgchart.min.js"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/portal.js"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/website_search.js"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/tree.jquery.js"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/social_group_heirarchy.js"/>

        </xpath>
    </template>

     <template id="assets_backend" name="allure theme assets"
              inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss"
                  href="/dcm_customization/static/src/scss/social.scss"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/dialog.js"></script>
            <script type="text/javascript" src="/dcm_customization/static/src/js/global_search.js"></script>
            <script type="text/javascript" src="/dcm_customization/static/src/js/menu.js"></script>
            <script type="text/javascript" src="/dcm_customization/static/src/js/view_dialogs.js"></script>
            <link rel="stylesheet"
              type="text/scss"
              href="/dcm_customization/static/src/scss/global_search.scss"/>
            <link rel="stylesheet"
              type="text/scss"
              href="/dcm_customization/static/src/scss/dialog.scss"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/social_group_org_chart.js"></script>
            <script type="text/javascript" src="/dcm_customization/static/src/js/social_group_fun_org_chart.js"></script>

            <script type="text/javascript" src="/dcm_customization/static/src/js/bit_kanban_comment.js"/>
            <script type="text/javascript" src="/dcm_customization/static/src/js/stream_post_kanban_controller.js"/>

            <script type="text/javascript" src="/dcm_customization/static/src/js/dashboard_chart_preview.js"/>
        </xpath>

    </template>
</odoo>
